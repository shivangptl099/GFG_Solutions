class Solution {
  public:
    vector<int> find3Numbers(vector<int> &arr) {
        if(arr.size() < 3) return {};
        vector<int> ans(3);
        for(int i = 0; i < arr.size() - 2; i++){
            ans[0] = arr[i];
            for(int j = i + 1; j < arr.size() - 1; j++){
                if(arr[j] > arr[i]){
                    ans[1] = arr[j];
                    for(int k = j + 1; k < arr.size(); k++){
                        if(arr[k] > arr[j]){
                            ans[2] = arr[k];
                            return ans;
                        }
                    }
                }
            }
        }
        return {};
    }
};
